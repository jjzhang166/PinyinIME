    balloon="true"
    height="20%p"
    key_type="0"
    repeat="false"
    skb_cache_flag="true"
    skb_template="@xml/skb_template1"
    width="25%p" >

    <row
        start_pos_x="0%p"
        start_pos_y="0%p"
        width="25%p" >
        <key id="6" />
