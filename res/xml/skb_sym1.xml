            code="4"
            height="20%p"
            icon="@drawable/tutorial_back"
            key_type="1" />
    </row>
    <row start_pos_x="0%p" >
        <keys
            codes="8|9|10"
            labels="1|2|3"
            splitter="|" />

        <key
            code="67"
            height="40%p"
            icon="@drawable/delete_icon"
            key_type="1"
            repeat="true"
            start_pos_y="20%p"
            width="25%p" />
    </row>
    <row start_pos_x="0%p" >
        <keys
            codes="11|12|13"
            labels="4|5|6"
            splitter="|" />
    </row>
    <row start_pos_x="0%p" >
        <keys
            codes="14|15|16"
            labels="7|8|9"
            splitter="|" />

        <key
            balloon="false"
            code="66"
            height="40%p"
            key_type="1"
            label="下一个"
            width="25%p" />
    </row>
    <row start_pos_x="0%p" >
        <key
            code="56"
            icon="@drawable/period_icon"
            key_type="0"
            label="。"
            width="25%p" />
        <key
            code="7"
            label="0"
            width="50%p" />
    </row>

